{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8080",
      "sslPort": 0
    }
  },
  "profiles": {
    // This profile is first in order for dotnet run to pick it up by default

    "SkiaDesktopTest (WinAppSDK Unpackaged)": {
      "commandName": "Project",
      "compatibleTargetFramework": "windows"
    },
    "SkiaDesktopTest (WinAppSDK Packaged)": {
      "commandName": "MsixPackage",
      "compatibleTargetFramework": "windows"
    },
    "SkiaDesktopTest (Desktop)": {
      "commandName": "Project",
      "compatibleTargetFramework": "desktop"
    },
    "SkiaDesktopTest (Desktop WSL2)": {
      "commandName": "WSL2",
      "commandLineArgs": "{ProjectDir}/bin/Debug/net9.0-desktop/SkiaDesktopTest.dll",
      "distributionName": "",
      "compatibleTargetFramework": "desktop"
    }

